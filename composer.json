{
    "name": "anytech/lightspeed-x-series-api",
    "type": "library",
    "description": "Lightspeed X-Series (formerly Vend) API client for PHP with built-in OAuth2 support",
    "keywords": [
        "lightspeed",
        "vend",
        "pos",
        "retail",
        "e-commerce",
        "api",
        "oauth2"
    ],
    "homepage": "https://github.com/anytech/lightspeed-x-series-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Kayne Middleton",
            "email": "kayne@anytech.ca",
            "homepage": "https://anytech.ca"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "LightspeedXSeries\\": "src/LightspeedXSeries"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LightspeedXSeries\\Tests\\": "tests"
        }
    },
    "minimum-stability": "stable"
}